-- spawners
function spawn_bullet(x, y, direction)
    local bullet = {
        x = x,
        y = y-5,
        dx = bullet_speed * direction,
        dy = 0,
        remove = false
    }
    add(bullets, bullet)
end

function spawn_stone(x, y)
    local stone = {
        x = x,
        y = y,
        dx = 0,
        dy = 0,
        remove = false
    }
    add(stones, stone)
end

function spawn_enemy(x, y, direction)
    local enemy = {
        x = x,
        y = y,
        dir = direction,
        appear = 0,
        dx = 0,
        dy = 0,
        remove = false
    }
    add(enemies, enemy)
end
