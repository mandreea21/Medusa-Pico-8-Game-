-- collisions
function is_wall(x, y)
    local tile_x = flr(x / 8)
    local tile_y = flr(y / 8)     
   	return fget(mget(tile_x, tile_y), 0)
 
end

function is_end(x, y)
    local tile_x = flr(x / 8)
    local tile_y = flr(y / 8)     
   	return fget(mget(tile_x, tile_y), 2)
 
end

function is_stone(x, y)
    for stone in all(stones) do
        if abs(stone.x - x) < 8 
        and abs(stone.y - y) < 8 then
            return true
        end
    end
    return false
end

function check_collision(x, y, dx, dy)
    
    if is_wall(x + dx, y + dy) or is_wall(x + 7 + dx, y + dy) or is_wall(x + dx, y + 7 + dy) or is_wall(x + 7 + dx, y + 7 + dy) then
        return true
    end
    
    if is_end(x + dx, y + dy) or is_wall(x + 7 + dx, y + dy) or is_wall(x + dx, y + 7 + dy) or is_wall(x + 7 + dx, y + 7 + dy) then
        
        _init()
        scene = "win"
        
        sfx(04)
    end
    
    if dy > 0 then
        if is_stone(x + dx, y + dy) or is_stone(x + 7 + dx, y + dy) then
            return true
        end
    end
    return false
end
