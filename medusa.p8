-- game
function game_on()
    player.dy += gravity
    player.on_ground = false

    -- player movement
    if btn(0) then 
        player.dx = -move_speed 
        player.facing_left = true
    end -- left
    if btn(1) then 
        player.dx = move_speed 
        player.facing_left = false
    end -- right
    if not btn(0) and not btn(1) then 
        player.dx = 0 
    end -- no horizontal movement

    -- horizontal collision detection
    player.x += player.dx
    if check_collision(player.x, player.y, player.dx, 0) then
        player.x -= player.dx
    end

    -- vertical collision detection
    player.y += player.dy
    if check_collision(player.x, player.y, 0, player.dy) then
        player.y -= player.dy
        player.dy = 0
        player.on_ground = true
    end

    -- jumping
    if player.on_ground and btnp(5) then -- button 5 is usually the space bar
        player.dy = jump_strength
        player.on_ground = false
    end
    
    -- shooting bullets
    if btnp(4) then 
        local bullet_dx = player.facing_left and -1 or 1
        spawn_bullet(player.x + 4, player.y + 4, bullet_dx)
    				sfx(00)
    end
    
    -- update bullets
    for bullet in all(bullets) do
        bullet.x += bullet.dx
        bullet.y += bullet.dy
        
        -- check for collision with enemies
        for enemy in all(enemies) do
            if abs(bullet.x - enemy.x) < 8 and abs(bullet.y - enemy.y) < 8 then
                -- mark bullet for removal
                bullet.remove = true
                player.score += 50
                
                spawn_stone(enemy.x, enemy.y)
                if enemy.appear < 3 then
                	enemy.appear += 1
                	enemy.y = enemy.y - 8
                else  	
                	--spawn_enemy(enemy.x, enemy.y - 8, 1, 1)
                	enemy.remove = true
                	del(enemies, enemy)
                end
                sfx(01)
                break
            end
        end
    end

    -- remove bullets marked for removal
    for i = #bullets, 1, -1 do
        if bullets[i].remove then
            del(bullets, bullets[i])
        end
    end
    
    -- update enemies
    for enemy in all(enemies) do
        enemy.x += enemy.dir
        if enemy.x < 40 or enemy.x > 80 then enemy.dir = -enemy.dir end
       
        if abs(player.x - enemy.x) < 8 and abs(player.y - enemy.y) < 8 then
            _init() 
            sfx(02)
        end
    end
    
   
    
    -- check if all treasures collected and player reached exit
    --if #treasures == 0 and abs(player.x - exit.x) < 8 and abs(player.y - exit.y) < 8 then
    --    _init()
    --end
end

function draw_game()
    cls()
    draw_map()
    
    -- draw player
    if player.facing_left then
        spr(1, player.x, player.y, 1, 1, true) 
    else
        spr(1, player.x, player.y)
    end

    --for t in all(treasures) do
    --    spr(2, t.x, t.y)
    --end
    
    for e in all(enemies) do
        if fly == 1 then
            if enemframe == 12 then
                fly = 0
            end
            enemframe += 1
        elseif fly == 0 then
            if enemframe == 11 then
                fly = 1
            end
            enemframe -= 1
        end
        spr(enemframe, e.x, e.y)
    end
    
    for bullet in all(bullets) do
        local sprite_attack = 6 + rnd(4)
        spr(sprite_attack, bullet.x, bullet.y)
    end
    
    for stone in all(stones) do
        spr(20, stone.x, stone.y)
    end
    
    print("score: " .. player.score, 50, 1, 10) -- display score at the top-left corner in color 7 (white)
end



