-- init
function _init()
				scene = "menu"
    load_map()
    player = {x = 20, y = 100, dx = 0, dy = 0, score = 0, on_ground = false, facing_left}
    gravity = 0.2
    jump_strength = -2.5
    move_speed = 2
    bullet_speed = 4
    bullets = {}
    --treasures = {{x = 40, y = 40}, {x = 80, y = 24}}
    enemies = {{x = 50, y = 102, dir = 1,appear = 1},
    											{x = 40, y = 28, dir = -1,appear = 1},
    											{x = 40, y = 68, dir = -1,appear = 1}}
    exit = {x = 120, y = 120}
    enemframe = 10
    fly = 1
    stones = {}
    level = 1
    music(0)
    
end

function _update()
			if scene == "menu" then
				menu_on()
				player.x = 20
				player.y = 100
			elseif scene=="game" then
			
    game_on()
   elseif scene=="win" then
    win_on()
   end
end

function _draw()
		 if scene=="menu" then
        draw_menu()
   elseif scene=="game" then
   					--camera(0, 18)

        draw_game()
   elseif scene=="win" then
   					draw_win()
   end 
end

function load_map()
    if scene=="menu" or scene=="win"then
        map(19, 0, 0, 0, 16, 16)
   elseif scene=="game" then
   
        map(0, 0, 0, 0, 16, 16)
   end
    
end



function draw_map()
    
    if scene=="menu" or scene=="win" then
        map(19, 0, 0, 0, 16, 16)
   elseif scene=="game" then
   					
        map(0, 0, 0, 0, 16, 16)
   end
   
end

